{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "xc-music",
  "version": "0.1.0",
  "identifier": "xcmusic.yiktllw.com",
  "build": {
    "frontendDist": "../dist_electron",
    "devUrl": "http://localhost:4321",
    "beforeDevCommand": "npm run serve",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "xc-music",
        "width": 1177,
        "height": 777,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "resources/**/*"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
