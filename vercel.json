{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist_electron" 
            }
        }
    ],
    "rewrites": [
        {
            "source": "/:match*",
            "destination": "https://netease-cloud-music-api-mauve.vercel.app/:match*"
        }
    ]
}