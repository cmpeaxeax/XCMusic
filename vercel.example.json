{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist_electron" 
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "https://your.NeteaseCloudMusicApi.deployment/:match*"
        }
    ]
}